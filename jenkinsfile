pipeline {
    agent any

    environment {
        flutter = "/Users/TBS/fvm/default/bin/flutter"
    }


    stages {
        stage('Flutter Doctor') {
            steps {
                withEnv(['PATH+EXTRA=/opt/flutter/bin']) {
                    sh '$flutter doctor -v'
                }
            }
        }

        stage('GIT PULL') {
            steps {
                git branch: 'master',
                url: 'https://github.com/lucas-castro-icts/android_jenkins.git'
            }
        }

    }
}
