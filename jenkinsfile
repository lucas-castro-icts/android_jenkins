pipeline {
    agent any
    environment {
        flutter = '/Users/TBS/fvm/default/bin/flutter'
    }
    stages {
        stage('GIT PULL') {
            steps {
                git branch: 'master',
                url: 'https://github.com/lucas-castro-icts/android_jenkins.git'
            }
        }

        stage('CLEAN') {
            steps {
                sh "flutter clean"
            }
        }

        stage('PUB GET') {
            steps {
                bat "${flutter} pub get"
            }
        }
    }
}
