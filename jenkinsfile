pipeline {
    agent any
    environment {
        PATH = "${env.PATH}:C:/Users/TBS/fvm/default/bin"
    }
    stages {
        stage('Checkout') {
            steps {
                checkout([$class: 'GitSCM', branches: [[name: '*/master']], userRemoteConfigs: [[url: 'https://github.com/lucas-castro-icts/android_jenkins.git']]])
            }
        }
        stage('Clean') {
            steps {
                sh 'flutter clean'
            }
        }
        stage('Pub Get') {
            steps {
                sh 'flutter pub get'
            }
        }
    }
}
